{
  "name": "universal-router-typesafe-urls",
  "version": "1.1.1",
  "description": "URL Generation for Universal Router",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "universal-router-typesafe-urls": "lib/index.js"
  },
  "scripts": {
    "build": "rimraf lib && tsc",
    "test": "jest",
    "format": "prettier . --check",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Relz/universal-router-typesafe-urls.git"
  },
  "keywords": [
    "universal-router",
    "url generation"
  ],
  "author": "Relz",
  "license": "ISC",
  "devDependencies": {
    "@types/glob": "7.2.0",
    "@types/jest": "28.1.6",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.3.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-regexp": "1.7.0",
    "eslint-plugin-sonarjs": "0.14.0",
    "eslint-plugin-unicorn": "43.0.2",
    "glob": "8.0.3",
    "jest": "28.1.3",
    "jest-html-reporters": "3.0.10",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.7",
    "typescript": "4.7.4"
  },
  "files": [
    "lib/**/*"
  ]
}
